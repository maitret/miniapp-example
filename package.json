{
    "name": "miniapp-example",
    "version": "0.0.1",
    "homepage": "https://yandex.github.io/miniapp-example/",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && node ./tools/generate-yandex-manifest.js",
        "deploy": "gh-pages -d build",
        "predeploy": "REACT_APP_API_HOST=/miniapp-example npm run build",
        "ci:build": "npm run build >& __reports/build.log",
        "ci:analyze-bundle": "mkdir __reports/bundle && source-map-explorer 'build/static/{js,css}/*.{js,css}' --html __reports/bundle/index.html"
    },
    "devDependencies": {
        "@piotr-cz/redux-persist-idb-storage": "1.0.2",
        "@reduxjs/toolkit": "1.1.0",
        "@types/node": "13.1.0",
        "@types/react": "16.9.11",
        "@types/react-dom": "16.9.4",
        "@types/react-redux": "7.1.5",
        "@types/react-router-dom": "5.1.2",
        "@types/redux-thunk": "2.1.0",
        "@types/throttle-debounce": "2.1.0",
        "date-fns": "1.30.1",
        "gh-pages": "2.1.1",
        "intersection-observer": "0.7.0",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-redux": "7.1.3",
        "react-router-dom": "5.1.2",
        "react-scripts": "3.2.0",
        "react-use": "13.12.2",
        "react-yandex-maps": "4.2.0",
        "redux-persist": "6.0.0",
        "redux-thunk": "2.3.0",
        "serve": "11.2.0",
        "source-map-explorer": "2.1.1",
        "throttle-debounce": "2.1.0",
        "typescript": "3.7.3",
        "whatwg-fetch": "3.0.0"
    },
    "browserslist": {
        "production": [
            "iOS >= 10",
            "Chrome >= 76"
        ],
        "development": [
            "iOS >= 10",
            "Chrome >= 76"
        ]
    }
}
